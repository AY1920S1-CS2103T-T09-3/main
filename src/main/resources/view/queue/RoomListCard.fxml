<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" fx:id="roomCard" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
  <HBox alignment="CENTER_LEFT" fx:id="doctorPane" prefHeight="40">
    <Label fx:id="doctorName" text="\$doctorName" styleClass="cell_big_label" />
  </HBox>
  <HBox alignment="CENTER_LEFT" fx:id="patientPane" prefHeight="40">
    <Label fx:id="patientName" text="\$patientName" styleClass="cell_big_label" />
  </HBox>
  <HBox fx:id="invisiblePane" prefHeight="40"/>
</VBox>
